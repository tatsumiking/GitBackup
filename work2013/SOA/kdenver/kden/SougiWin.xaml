        <Label Content="告別式日時" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Height="26" Margin="20,205,0,0" FontSize="14"/>
        <TextBox x:Name="txtSikiGMD" Text="" Margin="100,205,0,0" Width="230" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top" IsEnabled="False"/>
        <TextBox x:Name="txtSSikiHH" Text="" Margin="100,245,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnSSikiHH" Content="▼" Margin="140,245,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnSSikiHH_Click"/>
        <Label Content="時" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="165,245,0,0" FontSize="14"/>
        <TextBox x:Name="txtSSikiMin" Text="" Margin="195,245,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
