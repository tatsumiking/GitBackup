        <Label Content="～" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="290,245,0,0" FontSize="14"/>
        <TextBox x:Name="txtESikiHH" Text="" Margin="330,245,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnESikiHH" Content="▼" Margin="370,245,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnESikiHH_Click"/>
        <Label Content="時" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="395,245,0,0" FontSize="14"/>
        <TextBox x:Name="txtESikiMin" Text="" Margin="425,245,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnESikiMin" Content="▼" Margin="465,245,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnESikiMin_Click"/>
        <Label Content="分" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="490,245,0,0" FontSize="14"/>
        <Label Content="施工場所" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Height="26" Margin="20,285,0,0" FontSize="14"/>
        <TextBox x:Name="txtSekoubasyo" Text="" Margin="100,285,0,0" Width="230" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label Content="担当" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Height="26" Margin="20,325,0,0" FontSize="14"/>
        <TextBox x:Name="txtTantou" Text="" Margin="100,325,0,0" Width="230" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnPreView" Content="印刷" HorizontalAlignment="Left" Margin="195,370,0,0" VerticalAlignment="Top" Width="75" Click="btnPreView_Click"/>
        <Button x:Name="btnRet" Content="戻る" HorizontalAlignment="Left" Margin="295,370,0,0" VerticalAlignment="Top" Width="75" Click="btnRet_Click"/>
