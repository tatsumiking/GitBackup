<Window x:Class="kden.SougiWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SougiWin" Height="450" Width="550" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid Background="LightGoldenrodYellow">
        <Label Content="葬儀情報入力" Margin="0,10,0,0" Width="250" HorizontalContentAlignment="Center" Background="#FF80FF80" HorizontalAlignment="Center" Height="26" VerticalAlignment="Top"/>
        <Label Content="葬家" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Height="26" Margin="20,60,0,0" FontSize="14"/>
        <TextBox x:Name="txtSouke" Text="" Margin="100,60,0,0" Width="230" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top" IsEnabled="False"/>
        <Label Content="通夜日時" HorizontalAlignment="Left" VerticalAlignment="Top" Width="80" Height="26" Margin="20,110,0,0" FontSize="14"/>
        <TextBox x:Name="txtTuyaYY" Text="" Margin="100,110,0,0" Width="65" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label Content="年" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="165,110,0,0" FontSize="14"/>
        <TextBox x:Name="txtTuyaMM" Text="" Margin="195,110,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnTuyaMM" Content="▼" Margin="235,110,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnTuyaMM_Click"/>
        <Label Content="月" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="260,110,0,0" FontSize="14"/>
        <TextBox x:Name="txtTuyaDD" Text="" Margin="290,110,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnTuyaDD" Content="▼" Margin="330,110,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnTuyaDD_Click"/>
        <Label Content="日" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="355,110,0,0" FontSize="14"/>
        <TextBox x:Name="txtSTuyaHH" Text="" Margin="100,150,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnSTuyaHH" Content="▼" Margin="140,150,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnSTuyaHH_Click"/>
        <Label Content="時" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="165,150,0,0" FontSize="14"/>
        <TextBox x:Name="txtSTuyaMin" Text="" Margin="195,150,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
