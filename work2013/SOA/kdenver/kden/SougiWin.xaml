        <Label Content="分" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="260,150,0,0" FontSize="14"/>
        <Label Content="～" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="290,150,0,0" FontSize="14"/>
        <TextBox x:Name="txtETuyaHH" Text="" Margin="330,150,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnETuyaHH" Content="▼" Margin="370,150,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnETuyaHH_Click"/>
        <Label Content="時" HorizontalAlignment="Left" VerticalAlignment="Top" Width="25" Height="26" Margin="395,150,0,0" FontSize="14"/>
        <TextBox x:Name="txtETuyaMin" Text="" Margin="425,150,0,0" Width="40" Height="26" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnETuyaMin" Content="▼" Margin="465,150,0,0" Width="25" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Click="btnETuyaMin_Click"/>
