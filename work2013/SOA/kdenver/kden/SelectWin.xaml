<Window x:Class="kden.SelectWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SelectWin" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid Background="LightGoldenrodYellow">
        <ListBox x:Name="listSelect" HorizontalAlignment="Left" Height="250" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" PreviewMouseRightButtonDown="listSelect_PreviewMouseRightButtonDown" SelectionChanged="listSelect_SelectionChanged" />
    </Grid>
</Window>
